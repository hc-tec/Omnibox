{
  "name": "intelligent-panel-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist-electron/electron/main.js",
  "scripts": {
    "dev": "vite",
    "electron:dev": "concurrently \"npm run dev\" \"npm run electron:start\"",
    "electron:start": "wait-on tcp:localhost:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron -r esbuild-register electron/main.ts",
    "electron:prebuild": "tsc -b tsconfig.electron.json",
    "electron:build": "npm run build && npm run electron:prebuild && cross-env CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder",
    "build": "vite build",
    "postinstall": "patch-package",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/vue-table": "^8.15.5",
    "@vueuse/core": "^14.0.0",
    "axios": "^1.7.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "echarts": "^5.5.0",
    "lucide-vue-next": "^0.553.0",
    "marked": "^12.0.2",
    "pinia": "^2.1.7",
    "reka-ui": "^2.6.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.17",
    "tailwindcss-animate": "^1.0.7",
    "vue": "^3.4.38",
    "vue-echarts": "^6.7.2",
    "vue-grid-layout-v3": "^3.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.16.10",
    "@vitejs/plugin-vue": "^5.1.2",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^39.1.1",
    "electron-builder": "^26.0.12",
    "esbuild-register": "^3.5.0",
    "patch-package": "^8.0.1",
    "shadcn-vue": "^2.3.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.4.5",
    "vite": "^5.4.8",
    "wait-on": "^9.0.3"
  },
  "build": {
    "appId": "com.omni.panel",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "package.json"
    ],
    "win": {
      "icon": "build/icon.ico",
      "target": [
        "nsis"
      ]
    }
  }
}
